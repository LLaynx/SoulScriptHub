	PlrId = Frame.PlrId
	
	local UserId = game.Players.LocalPlayer.UserId
	local thumtype = Enum.ThumbnailType.AvatarBust
	local thumsize = Enum.ThumbnailSize.Size420x420
	local content = game.Players:GetUserThumbnailAsync(UserId, thumtype, thumsize)
	
	PlrImg.Image = content
	Name.Text = game.Players.LocalPlayer.Name
	UserName.Text = game.Players.LocalPlayer.DisplayName
	PlrId.Text = UserId
	
	Copy.MouseButton1Up:Connect(function()
		setclipboard("https://discord.gg/aSxrWzeBj8")
		game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Soul Script", Text = "Copied!"})
	end)
	
	
end
