local function IZHXRTK_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	MainButton = script.Parent.ButtonFrame.MainButton
	PositionButton = script.Parent.ButtonFrame.PositinButton
	GameButton = script.Parent.ButtonFrame.GameButton
	OutputButton = script.Parent.ButtonFrame.OutputButton
	ScriptButton = script.Parent.ButtonFrame.ScriptButton
		
	Frames = script.Parent.Parent
	
	MainButton.MouseButton1Up:Connect(function()
		Frames.MainFrame.Visible = true
		Frames.PositionFrame.Visible = false
		Frames.GameFrame.Visible = false
		Frames.OutputFrame.Visible = false
		Frames.ScriptFrame.Visible = false
	end)
	
	PositionButton.MouseButton1Up:Connect(function()
		Frames.MainFrame.Visible = false
		Frames.PositionFrame.Visible = true
		Frames.GameFrame.Visible = false
		Frames.OutputFrame.Visible = false
		Frames.ScriptFrame.Visible = false
	end)
	
	GameButton.MouseButton1Up:Connect(function()
		Frames.MainFrame.Visible = false
		Frames.PositionFrame.Visible = false
		Frames.GameFrame.Visible = true
		Frames.OutputFrame.Visible = false
		Frames.ScriptFrame.Visible = false
	end)
	
	OutputButton.MouseButton1Up:Connect(function()
		Frames.MainFrame.Visible = false
		Frames.PositionFrame.Visible = false
		Frames.GameFrame.Visible = false
		Frames.OutputFrame.Visible = true
		Frames.ScriptFrame.Visible = false
	end)
	
	ScriptButton.MouseButton1Up:Connect(function()
		Frames.MainFrame.Visible = false
		Frames.PositionFrame.Visible = false
		Frames.GameFrame.Visible = false
		Frames.OutputFrame.Visible = false
		Frames.ScriptFrame.Visible = true
	end)
end
coroutine.wrap(IZHXRTK_fake_script)()
local function TVSHNS_fake_script() -- PositionFrame.LocalScript 
	local script = Instance.new('LocalScript', PositionFrame)

	--Character Position
	
	Posittion = script.Parent.pos
	Generate = script.Parent.gen
	Copy = script.Parent.Copy
	
	Generate.MouseButton1Up:Connect(function()
		Posittion.Text = tostring(game.Players.LocalPlayer.Character.HumanoidRootPart.Position)
	end)
	
	Copy.MouseButton1Up:Connect(function()
		setclipboard(Posittion.Text)
		game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Soul Script", Text = "Copied!"})
	end)
	
	--Character Teleport
	
	TpButton = script.Parent.Tp
	TpTextBoxX = script.Parent.TpPositionX
	TpTextBoxY = script.Parent.TpPositionY
	TpTextBoxZ = script.Parent.TpPositionZ
	
	TpButton.MouseButton1Up:Connect(function()
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(TpTextBoxX.Text, TpTextBoxY.Text, TpTextBoxZ.Text)
	end)
	
end
coroutine.wrap(TVSHNS_fake_script)()
local function HYFRSBX_fake_script() -- MainFrame.LocalScript 
	local script = Instance.new('LocalScript', MainFrame)

	Frame = script.Parent
	
	PlrImg = Frame.PlayerImage
	UserName = Frame.UserName
	Name = Frame.Names
