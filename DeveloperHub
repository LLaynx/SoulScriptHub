local function VYSOOP_fake_script() -- OutputFrame_2.OutputScript 
	local script = Instance.new('LocalScript', OutputFrame_2)

	local colors = {
		[Enum.MessageType.MessageOutput] = BrickColor.new("White").Color;
		[Enum.MessageType.MessageInfo] = BrickColor.new("Bright blue").Color;
		[Enum.MessageType.MessageWarning] =	BrickColor.new("Bright yellow").Color;
		[Enum.MessageType.MessageError]	 = BrickColor.new("Bright red").Color;
	}
	local canvasX = 0
	
	repeat wait() until script.Parent:FindFirstChild'txt'
	
	local CreateLogOutput = function(output, messageType)
		local newLine = script.Parent.txt:clone()
		if messageType==nil then messageType=Enum.MessageType.MessageOutput end
		newLine.TextColor3 = colors[messageType]
		newLine.Position = UDim2.new(0, 5, 0, (#script.Parent.Consol:getChildren() * 15))
		newLine.Text = output
		newLine.Visible = true
		newLine.TextSize = 16
		newLine.Name = tick()
		newLine.Parent = script.Parent.Consol
		newLine.Size = UDim2.new(0, (#output * 10), 0, 15)
		canvasX = (canvasX > (#output * 10) and canvasX or (#output * 10))
		script.Parent.Consol.CanvasSize = UDim2.new(0, (canvasX + 10), 0, ((#script.Parent.Consol:getChildren() + 1) * 15))
	end
	game:getService("LogService").MessageOut:connect(function(output, messageType)CreateLogOutput(output, messageType) end)
	local x = game:GetService('LogService'):GetLogHistory()
	local z={}
	CreateLogOutput('Ouput Loaded',Enum.MessageType.MessageInfo)
	wait(1)
	CreateLogOutput('This Developer Console is made by SoulScript',Enum.MessageType.MessageInfo)
	for _,k in pairs(z) do
		CreateLogOutput(k[1])
	end
	
end
coroutine.wrap(VYSOOP_fake_script)()
