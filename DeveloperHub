local function UZND_fake_script() -- Main.LocalScript 
	local script = Instance.new('LocalScript', Main)

	script.Parent.Draggable = true
	script.Parent.Active = true
	script.Parent.Selectable = true
	
	game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Soul Script", Text = "Press p to open and close the Gui"})
	
	local plr = game.Players.LocalPlayer
	
	plr:GetMouse().KeyDown:Connect(function(key)
		if key == "p" then 
			script.Parent.Visible = not script.Parent.Visible
		end
	end)
	
	script.Parent.Close.MouseButton1Up:Connect(function()
		script.Parent.Visible = false
		game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Soul Script", Text = "Press p to open the Gui"})
	end)
end
coroutine.wrap(UZND_fake_script)()
