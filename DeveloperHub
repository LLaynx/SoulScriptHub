local function CRRHAZ_fake_script() -- ScriptFrame.LocalScript 
	local script = Instance.new('LocalScript', ScriptFrame)

	Spy = script.Parent.Spy
	Dex = script.Parent.Dex
	Admin = script.Parent.Admin
